{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node",
        "config": {
          "installCommand": "npm install",
          "buildCommand": "npm run dev",
          "outputDirectory": "public"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js" 
      }
    ]
  }
  